{
  "name": "guesslot-js",
  "version": "0.0.1",
  "description": "Guesslot JS Library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "guesslot",
    "defi",
    "ethereum"
  ],
  "scripts": {
    "lint": "prettier src/*.ts src/**/*.ts src/**/*.json --write",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "author": "Guesslot",
  "license": "MIT",
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false,
    "tabWidth": 2
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/strings": "^5.6.1",
    "@ethersproject/units": "^5.6.1",
    "@ethersproject/wallet": "^5.6.2",
    "@walletconnect/ethereum-provider": "^1.7.8",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "events": "^3.3.0",
    "tslib": "^2.4.0",
    "utf-8-validate": "^5.0.9"
  }
}
